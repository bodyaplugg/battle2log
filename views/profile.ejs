<%
  function parseTime(timestamp) {
      var time = Number(timestamp)
      var hours = Math.floor(time / 60 / 60)
      var minutes = Math.floor(time / 60) - (hours * 60)
      if (minutes < 10)
        minutes = String(0) + minutes;
      var seconds = time - (hours * 60 * 60 + minutes * 60)
      if (seconds < 10)
        seconds = String(0) + seconds;
      return hours + ':' + minutes + ':' + seconds
  }

  function parseDate(timestamp) {
    var joinDate = new Date(Number(timestamp) * 1000);

    var day = joinDate.getDate()
    if (day < 10)
      day = String(0) + day;

    var month = joinDate.getMonth() + 1
    if (month < 10)
      month = String(0) + month;


    var hours = joinDate.getHours()
    if (hours < 10)
      hours = String(0) + hours;

    var minutes = joinDate.getMinutes()
    if (minutes < 10)
      minutes = String(0) + minutes;

    var year = joinDate.getFullYear()
    return day + '-' + month + '-' + year + ' ' + hours + ':' + minutes
  }

  function parseLevel(level) {
    if (level == 3) return translation.PROFILE_EXPERT
    else if (level == 2) return translation.PROFILE_VETERAN
    else return translation.PROFILE_BASIC
  }

  function parseWeapon(id) {
    if (id == '11') return 'DAO-12'
    else if (id == '22') return 'G3A3'
    else if (id == '33') return 'MK3A1'
    else if (id == '44') return 'L85A1'
    else if (id == '55') return 'G36C'
    else if (id == '66') return 'PKM'
    else if (id == '77') return 'M95'
    else if (id == '444') return 'P90'
    else if (id == '88') return 'FN2000'
    else if (id == '99') return 'MP7'
    else if (id == '111') return 'G36E'
    else if (id == '222') return 'SCAR-L'
    else if (id == '333') return 'MG36'
    else if (id == '555') return 'L96A1'
  }

  function findLevel(id, awards) {
    var level = 0
    for (var i = 0; i < awards.length; i++) {
      if (awards[i].award == id)
        level = awards[i].level
    }
    return level
  }

  function parseBadge(id, awards) {
    if (awards) {
    for (var i = 0; i < awards.length; i++) {
      if (awards[i].award == id) {
        var level = findLevel(id, awards);
        %> <%=parseLevel(level)%>
           <div class="img_award awl1_<%=level%> awi<%=id%>"></div>
           <%=parseDate(awards[i].when)%>
           <br />
        <%
        return
      }
    }
    }
    %> -
       <div class="img_award awl1_0 awi<%=id%>"></div>
       <br />
    <%
  }
    function parseUnlock(id, unlocks) {
      for (var i = 0; i < unlocks.length; i++) {
        if (unlocks[i].id == id && unlocks[i].state == "s"){
          %>
             <div class="img_unlock ull_1 uli<%=unlocks[i].id%>"></div>
             <%=parseWeapon(id)%>
             <br />
          <%
          return
        }
      }
      %>
         <div class="img_unlock ull_0 uli<%=id%>"></div>
         <%=parseWeapon(id)%>
         <br />
      <%
    }

    function parseOtherBadge(id, awards) {
      if (awards){
      for (var i = 0; i < awards.length; i++) {
        if (awards[i].award == id) {
          var level = findLevel(id, awards);
          %>
             <div class="img_award awl1_<%=level%> awi<%=id%>"></div>
             <%=parseDate(awards[i].when)%>
             <%=translation.AWARDS[id]%>
             <br />
          <%
          return
        }
      }
      }
      %>
         <div class="img_award awl1_0 awi<%=id%>"></div>
         <%=translation.AWARDS[id]%>
      <%
    }

    function parseMedal(id, awards) {
      if (awards) {
      for (var i = 0; i < awards.length; i++) {
        if (awards[i].award == id) {
          %>
             <div class="img_award awl2_1 awi<%=id%>"></div>
             <div class=""><%=parseDate(awards[i].when)%></div>
             <span class="badge bg-red">x<%=awards[i].level%></span>
             <%=translation.AWARDS[id]%>
          <%
          return
        }
      }
      }
      %>
         <div class="img_award awl2_0 awi<%=id%>"></div>
         <%=translation.AWARDS[id]%>
      <%
    }

    function parseRibbon(id, awards) {
      if (awards) {
      for (var i = 0; i < awards.length; i++) {
        if (awards[i].award == id) {
          %>
             <div class="img_award awl3_1 awi<%=id%>"></div>
             <div class=""><%=parseDate(awards[i].when)%></div>
             <%=translation.AWARDS[id]%>
          <%
          return
        }
      }
      }
      %>
         <div class="img_award awl3_0 awi<%=id%>"></div>
         <%=translation.AWARDS[id]%>
      <%
    }
%>
